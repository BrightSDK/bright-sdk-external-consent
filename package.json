{
  "name": "bright-sdk-external-consent",
  "version": "1.0.14",
  "main": "webpack.config.js",
  "author": "Vladislav S",
  "license": "ISC",
  "description": "External Consent library for BrightSDK apps",
  "devDependencies": {
    "@bright-sdk/i18n": "https://brightsdk.github.io/packages/bright-sdk-i18n/latest.tgz",
    "@bright-sdk/tool-release-manager": "https://brightsdk.github.io/packages/bright-sdk-tool-release-manager/latest.tgz",
    "css-loader": "^7.1.2",
    "handlebars": "^4.7.8",
    "handlebars-loader": "^1.7.3",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.12",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "bright-sdk-keycode-parser": "https://brightsdk.github.io/packages/bright-sdk-keycode-parser/latest.tgz",
    "core-js": "^3.41.0"
  },
  "scripts": {
    "build": "npx webpack --mode production",
    "build-debug": "npx webpack --mode development",
    "build:i18n": "bash update_i18n.sh",
    "release": "npm run build && npx release-manager",
    "release-debug": "npm run build-debug && npx release-manager"
  },
  "buildInfo": {
    "outputs": [
      "dist/consent.bundle.js - Main distribution bundle for releases",
      "examples/dist/consent.bundle.js - Copy for local example development"
    ]
  }
}
